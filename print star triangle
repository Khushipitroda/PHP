//Write a PHP programs to print star triangle.
//CODE:
<?php
echo"1st</br>";
for($i=5;$i>0;$i--){
	for($j=0;$j<$i;$j++){
	echo"*";}
	echo"</br>";
}	
echo"</br>";
echo"2nd";
for($i=0;$i<=5;$i++){
	for($j=0;$j<$i;$j++){
	echo"*";}
	echo"</br>";
}	
echo"</br>";
echo"3rd</br>";
for($i=5;$i>0;$i--){
	for($j=0;$j<5;$j++){
	echo"*";}
	echo"</br>";
}
